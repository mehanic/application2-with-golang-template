<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Stock Data</title>
    <style>
        table { border-collapse: collapse; width: 100%; }
        th, td { border: 1px solid #ccc; padding: 8px; text-align: center; }
        th { background-color: #f4f4f4; }
    </style>
</head>
<body>
<h1>Stock Data Table</h1>
<table>
    <thead>
        <tr>
            <th>Date</th>
            <th>Open</th>
            <th>High</th>
            <th>Low</th>
            <th>Close</th>
            <th>Volume</th>
            <th>Adj Close</th>
        </tr>
    </thead>
    <tbody>
        {{ range . }}
        <tr>
            <td>{{ .Date.Format "2006-01-02" }}</td>
            <td>{{ .Open }}</td>
            <td>{{ .High }}</td>
            <td>{{ .Low }}</td>
            <td>{{ .Close }}</td>
            <td>{{ .Volume }}</td>
            <td>{{ .AdjClose }}</td>
        </tr>
        {{ end }}
    </tbody>
</table>
</body>
</html>

